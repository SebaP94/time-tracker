{
  "projectType": "library",
  "root": "libs/tasks",
  "sourceRoot": "libs/tasks/src",
  "prefix": "time-tracker",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/tasks"],
      "options": {
        "project": "libs/tasks/ng-package.json",
        "tailwindConfig": "libs/tasks/tailwind.config.js"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/tasks/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/tasks/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/tasks"],
      "options": {
        "jestConfig": "libs/tasks/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/tasks/src/**/*.ts",
          "libs/tasks/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
